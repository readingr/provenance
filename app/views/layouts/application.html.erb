<!DOCTYPE html>
<html>
<head>
  <title>Provenance</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <% if @downloaded_datum %>
  <%= prov(@downloaded_datum) %>
  <%end %>
</head>
<body>
<%= render 'layouts/header' %>

  <div class="container">
    <%= yield %>
    <%= render 'layouts/footer' %>
  </div>
</body>
</html>
